{
    "name": "api",
    "version": "0.0.1",
    "description": "",
    "author": "Arnob Roy",
    "private": true,
    "license": "BSD-3-Clause",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "LOG_LEVEL=debug nest start --watch --preserveWatchOutput | yarn pino-pretty",
        "start:debug": "nest start --debug --watch --preserveWatchOutput",
        "start:prod": "node dist/main",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.5",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.5",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.5",
        "@nestjs/platform-socket.io": "^9.0.5",
        "@nestjs/websockets": "^9.0.5",
        "@socket.io/admin-ui": "^0.4.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "dotenv": "^16.0.0",
        "ioredis": "^5.2.1",
        "lodash": "^4.17.21",
        "mongodb": "4.9.1",
        "nanoid": "^3.3.4",
        "nestjs-pino": "^3.0.0",
        "passport": "^0.6.0",
        "passport-http-bearer": "^1.0.1",
        "passport-local": "^1.0.0",
        "pino-http": "^8.1.1",
        "pino-pretty": "^9.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "7.5.6",
        "ulid": "^2.3.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.5",
        "@types/express": "^4.17.13",
        "@types/jest": "29.1.1",
        "@types/lodash": "^4.14.182",
        "@types/node": "16.11.56",
        "@types/passport": "^1.0.7",
        "@types/passport-http-bearer": "^1.0.37",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "jest": "29.1.2",
        "source-map-support": "^0.5.20",
        "supertest": "6.3.0",
        "ts-jest": "^29.0.0",
        "ts-loader": "9.4.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "coveragePathIgnorePatterns": [
            ".module.ts",
            "<rootDir>/adapters/",
            "<rootDir>/config/",
            "<rootDir>/dto/",
            "<rootDir>/models/",
            "<rootDir>/typescript/",
            "<rootDir>/constants/",
            "main.ts",
            "database.service.ts",
            "repl.ts"
        ],
        "moduleNameMapper": {
            "@/(.*)": "<rootDir>/$1"
        },
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
